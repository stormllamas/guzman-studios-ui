{
  "semi": true,
  "singleQuote": false,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "arrowParens": "always",
  "printWidth": 80,
  "importOrder": [
    "^(react(/|-)(.*)$)|^(react$)",
    "<THIRD_PARTY_MODULES>",
    "types/(.*)$",
    "configs/(.*)$",
    "constants/(.*)$",
    "components/(.*)$",
    "store/(.*)$",
    "customHooks/(.*)$",
    "services/(.*)$",
    "utils/(.*)$",
    "^(?!(.*)assets/)[./](.*)$",
    "assets/"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}